#
# Complete the 'miniMaxSum' function below.
#
# The function accepts INTEGER_ARRAY arr as parameter.
#

def miniMaxSum(arr):
    # Write your code here only wants 4 of 5 elements
    
    #sort the array and just get the first 4 and last 4 
    
    arr.sort() 
    print(sum(arr[:len(arr) - 1]), sum(arr[:0:-1]))
    
             
        

if __name__ == '__main__':

    arr = list(map(int, input().rstrip().split()))

    miniMaxSum(arr)
