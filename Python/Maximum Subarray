class Solution(object):
    def maxSubArray(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        l = [0 for i in range(len(nums))]
        l[0] = nums[0]
        for i in range(1,len(nums)):
            l[i] = max(l[i-1]+nums[i],nums[i])
     
        return max(l)
