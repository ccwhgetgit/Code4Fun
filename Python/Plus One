class Solution(object):
    def plusOne(self, digits):
        """
        :type digits: List[int]
        :rtype: List[int]
        """
        string = ""
        for i in range(len(digits)): 
            string += str(digits[i])
            
        number = int(string) 
        
        number = number + 1 
        
        converted = str(number) 
        
        output = [] 
        
        for i in range(len(converted)):
            output.append(converted[i])
            
        return output
