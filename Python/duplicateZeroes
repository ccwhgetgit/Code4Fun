class Solution(object):
    def duplicateZeros(self, arr):
        """
        :type arr: List[int]
        :rtype: None Do not return anything, modify arr in-place instead.
        """
        
            # create our incrementor
        i = 0

        # loop through all dynamic elements
        while i < len(arr)-1:
            
            if arr[i]==0:
                arr.pop()
                arr.insert(i+1, 0)
                i += 1

    
            i += 1
