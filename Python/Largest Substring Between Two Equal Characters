class Solution(object):
    def maxLengthBetweenEqualCharacters(self, s):
        """
        :type s: str
        :rtype: int
        """
        
        count = 0 
        duplicates = False
        global_count = 0 
        
        for i in range(0, len(s)): 
            count = 0 
            for j in range(i + 1, len(s)): 
                if s[i] == s[j]: 
                    count = (j - i - 1)
                    duplicates = True
            
            if count > global_count: 
                global_count = count
        
        if duplicates == False: 
            global_count = -1
            
        return global_count
                    
                    
